# Select tools can be used to rename columns as well as
# pick them out of a data frame.
[input: './data/right.parquet' table="tbl1"] | [select: 'id:id_r,email,title] > df;

(
  left=[input: './data/left.parquet' table="tbl2"] | [select: 'id:id,first_name,last_name'],
  right=df
)
  | [join: type='inner' left='id' right='id_r']
  | [print: 10];
