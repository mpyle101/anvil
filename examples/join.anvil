# Select tools can be used to rename columns as well as
# pick them out of a data frame.
[input: './data/left.parquet'] | [select: 'id,first_name,last_name'] > L;

[join:
  L,
  ([input: './data/right.parquet'] | [select: 'id:id_r,email,title']),
  type='inner',
  cols_lt='id',
  cols_rt='id_r'
] | [print: 10];
